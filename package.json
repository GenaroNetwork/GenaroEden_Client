{
    "name": "genaro-eden",
    "productName": "Genaro Eden",
    "version": "1.2.1",
    "author": "Genaro Network",
    "description": "Genaro Eden - A Trustworthy Internet for Everyone",
    "license": "MIT",
    "main": "./dist/electron/main.js",
    "build": {
        "linux": {
            "target": [
                {
                    "target": "AppImage",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "directories": {
            "output": "./build"
        }
    },
    "scripts": {
        "build": "node .electron-vue/build.js",
        "build:darwin": "cross-env BUILD_TARGET=darwin node .electron-vue/build.js",
        "build:linux": "cross-env BUILD_TARGET=linux node .electron-vue/build.js",
        "build:mas": "cross-env BUILD_TARGET=mas node .electron-vue/build.js",
        "build:win32": "cross-env BUILD_TARGET=win32 node .electron-vue/build.js",
        "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
        "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
        "dev": "node .electron-vue/dev-runner.js",
        "pack": "npm run pack:main && npm run pack:renderer",
        "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
        "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
        "dist": "npm run pack && electron-builder"
    },
    "dependencies": {
        "@fortawesome/fontawesome": "^1.1.2",
        "@fortawesome/fontawesome-free-brands": "^5.0.4",
        "@fortawesome/fontawesome-free-solid": "^5.0.4",
        "@fortawesome/vue-fontawesome": "0.0.22",
        "axios": "^0.17.1",
        "bip39": "^2.5.0",
        "clone": "^2.1.1",
        "crypto-js": "^3.1.9-1",
        "element-ui": "^2.2.0",
        "eth-lightwallet": "^3.0.1",
        "ethereumjs-tx": "^1.3.3",
        "ethereumjs-wallet": "^0.6.0",
        "human-size": "^1.1.0",
        "jdenticon": "^1.8.0",
        "keccak": "^1.4.0",
        "keytar": "^4.1.0",
        "lowdb": "^1.0.0",
        "material-design-icons": "^3.0.1",
        "moment": "^2.20.1",
        "qrcode": "^1.2.0",
        "scrypt": "^6.0.3",
        "secp256k1": "^3.5.0",
        "storj": "github:seekerlee/node-libstorj",
        "tether-shepherd": "^1.8.1",
        "uuid": "^3.1.0",
        "vue": "^2.3.3",
        "vue-electron": "^1.0.6",
        "vue-i18n": "^7.4.2",
        "vue-router": "^3.0.1",
        "vuex": "^3.0.1",
        "web3": "1.0.0-beta.29"
    },
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-loader": "^7.1.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.24.1",
        "babili-webpack-plugin": "^0.1.2",
        "cfonts": "^1.1.3",
        "chalk": "^2.1.0",
        "copy-webpack-plugin": "^4.0.1",
        "cross-env": "^5.0.5",
        "css-loader": "^0.28.4",
        "del": "^3.0.0",
        "devtron": "^1.4.0",
        "electron": "^1.8.3",
        "electron-builder": "^20.5.1",
        "electron-debug": "^1.4.0",
        "electron-devtools-installer": "^2.2.0",
        "electron-packager": "^11.0.0",
        "electron-rebuild": "^1.6.0",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^1.1.6",
        "html-webpack-plugin": "^2.30.1",
        "less": "^2.7.3",
        "less-loader": "^4.0.5",
        "multispinner": "^0.2.1",
        "node-loader": "^0.6.0",
        "style-loader": "^0.20.1",
        "url-loader": "^0.6.2",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^14.1.1",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.4.2",
        "webpack": "^3.5.2",
        "webpack-dev-server": "^2.7.1",
        "webpack-hot-middleware": "^2.18.2"
    }
}
